model cms_roles {
  id                                   Int                                    @id @default(autoincrement())
  role_key                             String                                 @unique(map: "role_key") @db.VarChar(64)
  role_name                            String                                 @db.VarChar(120)
  is_active                            Boolean                                @default(true)
  created_at                           DateTime                               @default(now()) @db.DateTime(0)
  updated_at                           DateTime                               @default(now()) @db.DateTime(0)
  cms_role_item_transition_permissions cms_role_item_transition_permissions[]
  cms_role_permissions                 cms_role_permissions[]
  cms_user_roles                       cms_user_roles[]
}

model cms_permissions {
  id                   Int                              @id @default(autoincrement())
  permission_key       String                           @unique(map: "permission_key") @db.VarChar(120)
  permission_name      String                           @db.VarChar(180)
  permission_scope     cms_permissions_permission_scope @default(action)
  created_at           DateTime                         @default(now()) @db.DateTime(0)
  cms_role_permissions cms_role_permissions[]
}

model cms_role_permissions {
  role_id         Int
  permission_id   Int
  allowed         Boolean         @default(true)
  created_at      DateTime        @default(now()) @db.DateTime(0)
  cms_permissions cms_permissions @relation(fields: [permission_id], references: [id], onDelete: Cascade, map: "fk_cms_role_permissions_permission")
  cms_roles       cms_roles       @relation(fields: [role_id], references: [id], onDelete: Cascade, map: "fk_cms_role_permissions_role")

  @@id([role_id, permission_id])
  @@index([permission_id], map: "fk_cms_role_permissions_permission")
}

model cms_user_roles {
  user_id    Int
  role_id    Int
  is_primary Boolean   @default(true)
  created_at DateTime  @default(now()) @db.DateTime(0)
  updated_at DateTime  @default(now()) @db.DateTime(0)
  cms_roles  cms_roles @relation(fields: [role_id], references: [id], map: "fk_cms_user_roles_role")
  ag_users   ag_users  @relation(fields: [user_id], references: [user_id], map: "fk_cms_user_roles_user")

  @@id([user_id, role_id])
  @@index([user_id], map: "idx_cms_user_roles_user")
  @@index([role_id], map: "idx_cms_user_roles_role")
}

model cms_user_permission_overrides {
  id             BigInt   @id @default(autoincrement())
  user_id        Int
  permission_key String   @db.VarChar(120)
  allowed        Boolean
  reason         String?  @db.VarChar(255)
  created_at     DateTime @default(now()) @db.DateTime(0)
  updated_at     DateTime @default(now()) @db.DateTime(0)

  @@unique([user_id, permission_key], map: "uq_cms_user_permission_override")
}

model cms_order_item_statuses {
  id                                                                                                                Int                                     @id @default(autoincrement())
  status_key                                                                                                        String                                  @unique(map: "status_key") @db.VarChar(64)
  status_label                                                                                                      String                                  @db.VarChar(120)
  status_order                                                                                                      Int
  customer_bucket                                                                                                   cms_order_item_statuses_customer_bucket
  is_terminal                                                                                                       Boolean                                 @default(false)
  is_active                                                                                                         Boolean                                 @default(true)
  created_at                                                                                                        DateTime                                @default(now()) @db.DateTime(0)
  cms_order_item_status_transitions_cms_order_item_status_transitions_from_status_idTocms_order_item_statuses       cms_order_item_status_transitions[]     @relation("cms_order_item_status_transitions_from_status_idTocms_order_item_statuses")
  cms_order_item_status_transitions_cms_order_item_status_transitions_to_status_idTocms_order_item_statuses         cms_order_item_status_transitions[]     @relation("cms_order_item_status_transitions_to_status_idTocms_order_item_statuses")
  cms_role_item_transition_permissions_cms_role_item_transition_permissions_from_status_idTocms_order_item_statuses cms_role_item_transition_permissions[]  @relation("cms_role_item_transition_permissions_from_status_idTocms_order_item_statuses")
  cms_role_item_transition_permissions_cms_role_item_transition_permissions_to_status_idTocms_order_item_statuses   cms_role_item_transition_permissions[]  @relation("cms_role_item_transition_permissions_to_status_idTocms_order_item_statuses")
}

model cms_order_item_status_transitions {
  id                                                                                                BigInt                  @id @default(autoincrement())
  from_status_id                                                                                    Int
  to_status_id                                                                                      Int
  is_active                                                                                         Boolean                 @default(true)
  created_at                                                                                        DateTime                @default(now()) @db.DateTime(0)
  cms_order_item_statuses_cms_order_item_status_transitions_from_status_idTocms_order_item_statuses cms_order_item_statuses @relation("cms_order_item_status_transitions_from_status_idTocms_order_item_statuses", fields: [from_status_id], references: [id], onDelete: Cascade, map: "fk_cms_transition_from")
  cms_order_item_statuses_cms_order_item_status_transitions_to_status_idTocms_order_item_statuses   cms_order_item_statuses @relation("cms_order_item_status_transitions_to_status_idTocms_order_item_statuses", fields: [to_status_id], references: [id], onDelete: Cascade, map: "fk_cms_transition_to")

  @@unique([from_status_id, to_status_id], map: "uq_cms_order_item_transition")
  @@index([to_status_id], map: "fk_cms_transition_to")
}

model cms_role_item_transition_permissions {
  id                                                                                                   BigInt                  @id @default(autoincrement())
  role_id                                                                                              Int
  from_status_id                                                                                       Int
  to_status_id                                                                                         Int
  can_transition                                                                                       Boolean                 @default(true)
  requires_tracking_number                                                                             Boolean                 @default(false)
  created_at                                                                                           DateTime                @default(now()) @db.DateTime(0)
  cms_order_item_statuses_cms_role_item_transition_permissions_from_status_idTocms_order_item_statuses cms_order_item_statuses @relation("cms_role_item_transition_permissions_from_status_idTocms_order_item_statuses", fields: [from_status_id], references: [id], onDelete: Cascade, map: "fk_cms_role_transition_from")
  cms_roles                                                                                            cms_roles               @relation(fields: [role_id], references: [id], onDelete: Cascade, map: "fk_cms_role_transition_role")
  cms_order_item_statuses_cms_role_item_transition_permissions_to_status_idTocms_order_item_statuses   cms_order_item_statuses @relation("cms_role_item_transition_permissions_to_status_idTocms_order_item_statuses", fields: [to_status_id], references: [id], onDelete: Cascade, map: "fk_cms_role_transition_to")

  @@unique([role_id, from_status_id, to_status_id], map: "uq_cms_role_transition")
  @@index([from_status_id], map: "fk_cms_role_transition_from")
  @@index([to_status_id], map: "fk_cms_role_transition_to")
}

model order_product_status_history {
  id                       BigInt   @id @default(autoincrement())
  order_product_id         Int
  order_id                 Int
  from_status_id           Int?
  to_status_id             Int
  changed_by_user_id       Int?
  tracking_number_snapshot String?  @db.VarChar(255)
  note                     String?  @db.Text
  changed_at               DateTime @default(now()) @db.DateTime(0)

  @@index([order_product_id], map: "idx_ops_history_order_product")
  @@index([order_id], map: "idx_ops_history_order")
  @@index([changed_at], map: "idx_ops_history_changed_at")
}

